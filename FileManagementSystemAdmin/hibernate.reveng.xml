<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="T_File" schema="dbo" catalog="FileManagementSystemDB"
		class="model.TFile">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="V_File" schema="dbo" catalog="FileManagementSystemDB"
		class="model.VFile">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_Role" schema="dbo" catalog="FileManagementSystemDB"
		class="model.TRole">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_User" schema="dbo" catalog="FileManagementSystemDB"
		class="model.TUser">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="V_User" schema="dbo" catalog="FileManagementSystemDB"
		class="model.VUser">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_Department" schema="dbo"
		catalog="FileManagementSystemDB" class="model.TDepartment">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="T_FileType" schema="dbo"
		catalog="FileManagementSystemDB" class="model.TFileType">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="T_Authority" schema="dbo" catalog="FileManagementSystemDB"
	class="model.TAuthority">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>